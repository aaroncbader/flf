gfortran -fbacktrace -fdefault-real-8 -fdefault-double-8 -O2 -c coil_module.f90
gfortran -fbacktrace -fdefault-real-8 -fdefault-double-8 -O2 -c read_coils.f90
gfortran -fbacktrace -fdefault-real-8 -fdefault-double-8 -O2 -c compute_bs.f90
gfortran -fbacktrace -fdefault-real-8 -fdefault-double-8 -O2 -c dlsode.f
gfortran -fbacktrace -fdefault-real-8 -fdefault-double-8 -O2 follow_field.f90 coil_module.o read_coils.o compute_bs.o dlsode.o -o follow_field 
